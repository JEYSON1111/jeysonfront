ss').format('YYYY-MM-DD HH:mm:ss');\n      if (fecha_actual < item.fecha_inicio || fecha_actual > item.fecha_fin) {\n        me.$vs.notify({\n          text: 'Este juego aún no está disponible. Verifique las fechas de inicio y fin.',\n          color: 'warning',\n          iconPack: 'feather',\n          icon: 'icon-alert-triangle'\n        });\n        return;\n      }\n      switch (me.id_tipo_juego) {\n        case '1':\n          me.url_juego = 'ponchado';\n          break;\n        case '2':\n          me.url_juego = 'sopa_letras';\n          break;\n        default:\n          break;\n      }\n      var link = window.location.host + '/' + me.url_juego + '/' + item.id_juego;\n      var aux = document.createElement(\"input\");\n      aux.setAttribute(\"value\", link);\n      document.body.appendChild(aux);\n      aux.select();\n      try {\n        var status = document.execCommand('copy');\n        if (!status) {\n          me.$vs.notify({\n            text: 'Error al copiar el link',\n            color: 'warning',\n            iconPack: 'feather',\n            icon: 'icon-alert-triangle'\n          });\n        } else {\n          me.$vs.notify({\n            text: 'Link copiado',\n            color: 'success',\n            iconPack: 'feather',\n            icon: 'icon-check'\n          });\n        }\n      } catch (err) {\n        console.log('No se pudo copiar');\n      }\n      document.body.removeChild(aux);\n    }\n  }\n};",{"version":3,"names":["flatPickr","Vue","axios","vSelect","moment","$","data","juegos","usuario","tabActivo","id_juego","id_tipo_juego","nombre_tipo_juego","juegoElimin","activePromptEliminar","url_juego","components","created","me","JSON","parse","localStorage","getItem","mounted","getJuegos","methods","$vs","loading","formData","FormData","append","idusuario","$http","post","$server_url","then","res","items","close","catch","error","console","log","irCrearjuego","item","setItem","stringify","$router","push","openConfirmEliminar","el