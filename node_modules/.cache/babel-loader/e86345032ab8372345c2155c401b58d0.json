iculado: '',\n      PagosPendientes: false,\n      debeEstudiante: 0,\n      //para subir archivos,\n      attachments: [],\n      form: new FormData(),\n      errorImage: 'no',\n      tamanioError: \"no\",\n      archivoName: '',\n      urlImage: ''\n    };\n  },\n  components: {\n    'v-select': vSelect,\n    pensionesCobrar: pensionesCobrar,\n    DataEstudiante: DataEstudiante\n  },\n  created: function created() {\n    var me = this;\n    me.usuario = JSON.parse(localStorage.getItem(\"usuario\"));\n    me.getPeriodoInstitucion();\n    me.getDataEstudiante();\n    $('#btnUpdateDatos').css('display', 'block');\n  },\n  methods: {\n    //para cambiar de grupo al estudiante\n    //para obtener la informacion del estudiante\n    getDataEstudiante: function getDataEstudiante() {\n      var me = this;\n      me.$vs.loading();\n      this.$http.get(this.$server_url + 'estudiante/matricula?cedula=' + me.usuario.cedula + '&institucion_id=' + me.usuario.institucion_idInstitucion).then(function (res) {\n        me.estudiante = res.data[0];\n        me.$vs.loading.close();\n        me.getNivel();\n        me.getEstadoMatricula();\n        me.validarPagosPuntuales();\n      }).catch(function (error) {\n        console.log(error + ' error');\n        me.$vs.loading.close();\n      });\n    },\n    getNivel: function getNivel() {\n      var me = this;\n      var periodo = localStorage.getItem('periodo_id');\n      var institucion = me.usuario.institucion_idInstitucion;\n      var nivel = parseInt(me.estudiante.orden) + 1;\n      this.$http.get(this.$server_url + 'estudiante/matricula?NivelActual=' + nivel + '&periodo_id=' + periodo + '&institucion_id=' + institucion).then(function (res) {\n        if (me.estudiante.update_datos == 1 || me.estudiante.update_datos == 0) {\n          if (res.data.message) {\n            me.$vs.notify({\n              text: res.data.message,\n              color: 'warning',\n              time: 8000,\n              iconPack: 'feather',\n           