();\r\n            this.$http.get(this.$server_url+'reporte_redaccion/'+me.fecha_ini+'/'+me.fecha_fin)\r\n            .then(function (res) {\r\n                me.redactores = res.data\r\n                me.$vs.loading.close()\r\n            })\r\n        },\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n@import \"@/assets/scss/vuexy/apps/simple-calendar.scss\";\r\n</style>\r\n\r\n<style>\r\n.btn_estado{\r\n  padding: 5px 15px 5px 15px !important;\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,OAAAA,OAAA;AACA,OAAAC,SAAA;AACA;AACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,OAAA;MACAC,UAAA;MACAC,SAAA;MACAC,SAAA;MACAC,oBAAA;QACAC,UAAA;QACAC,UAAA;MACA;IACA;EACA;EACAC,UAAA;IACAT,SAAA,EAAAA,SAAA;IACA,YAAAD;EACA;EACAW,OAAA,WAAAA,QAAA;IACA,IAAAC,EAAA;IACA,KAAAT,OAAA,GAAAU,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;EACA;EACAC,OAAA;IACAC,UAAA,WAAAA,WAAA;MACA,IAAAN,EAAA;MACA,IAAAA,EAAA,CAAAP,SAAA,UAAAO,EAAA,CAAAN,SAAA;QACAM,EAAA,CAAAO,GAAA,CAAAC,MAAA;UAAAC,IAAA;UAAAC,KAAA;UAAAC,QAAA;UAAAC,IAAA;QAAA;QACA;MACA;MACAZ,EAAA,CAAAO,GAAA,CAAAM,OAAA;MACA,KAAAC,KAAA,CAAAC,GAAA,MAAAC,WAAA,0BAAAhB,EAAA,CAAAP,SAAA,SAAAO,EAAA,CAAAN,SAAA,EACAuB,IAAA,WAAAC,GAAA;QACAlB,EAAA,CAAAR,UAAA,GAAA0B,GAAA,CAAA5B,IAAA;QACAU,EAAA,CAAAO,GAAA,CAAAM,OAAA,CAAAM,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]}]}