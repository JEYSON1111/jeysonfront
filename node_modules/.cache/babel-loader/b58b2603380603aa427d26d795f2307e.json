natura;\n      me.asignatura.area = {\n        idarea: data.area_idarea,\n        nombrearea: data.nombrearea\n      };\n      me.asignatura.nivel = {\n        idnivel: data.nivel_idnivel,\n        nombrenivel: data.nombrenivel\n      };\n      me.asignatura.tipo_asignatura = data.tipo_asignatura;\n    },\n    openConfirm: function openConfirm(idasignatura, estado) {\n      var me = this;\n      me.idasignatura = idasignatura;\n      me.estado = estado;\n      this.$vs.dialog({\n        type: 'confirm',\n        color: 'danger',\n        title: \"Confirmar\",\n        text: 'Esta seguro de cambiar de estado',\n        accept: this.getEliminar\n      });\n    },\n    getEliminar: function getEliminar() {\n      var me = this;\n      this.$http.get(this.$server_url + \"cambiarEstadoAsignatura?asignatura_id=\" + me.idasignatura + '&estado=' + me.estado).then(function (response) {\n        me.getAdmin();\n      }).catch(function (error) {\n        me.$vs.notify({\n          text: 'No se pudo cambiar de estado',\n          color: 'danger',\n          iconPack: 'feather',\n          icon: 'icon-alert-triangle'\n        });\n      });\n    },\n    limpiar: function limpiar() {\n      var me = this;\n      me.asignatura.nombreasignatura = \"\";\n      me.asignatura.nivel = \"\";\n      me.asignatura.area = \"\";\n    }\n  }\n};",{"version":3,"names":["Vue","axios","use","vSelect","data","asignaturas","popupAddAsignatura","popupEditAsignatura","asignatura","nombreasignatura","area","nivel","tipo_asignatura","update","id","areas","niveles","idasignatura","estado","permisoRoot","usuario","components","mounted","JSON","parse","localStorage","getItem","getAdmin","getAreas","getNiveles","getUserRoot","methods","me","$http","get","$server_url","idusuario","then","res","status","catch","error","console","log","response","$vs","loading","close","guardarAsignatura","trim","notify","text","color","iconPack","icon","formData","FormData","append","idarea","idnivel","post","limpiar","ope