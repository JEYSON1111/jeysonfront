 type: Number,\n      default: 0\n    },\n    btnBloquear: {\n      type: Number,\n      default: 0\n    },\n    ifFechaEstudiante: {\n      type: Number,\n      default: 0\n    },\n    openModal: {\n      type: Number,\n      default: 0\n    },\n    btnAddStorage: {\n      type: Number,\n      default: 0\n    }\n  },\n  computed: {\n    SumaCodigosActivos: function SumaCodigosActivos() {\n      var me = this;\n      var datos = me.arregloCodigos.filter(function (p) {\n        return p.prueba_diagnostica == 0;\n      });\n      var resultado = datos.length;\n      return resultado;\n    },\n    SumaCodigosDiagnostico: function SumaCodigosDiagnostico() {\n      var me = this;\n      var datos = me.arregloCodigos.filter(function (p) {\n        return p.prueba_diagnostica == 1;\n      });\n      var resultado = datos.length;\n      return resultado;\n    }\n  },\n  data: function data() {\n    return {\n      usuario: [],\n      devoluciones: [],\n      popupReturns: false,\n      titleReturns: '',\n      //variables para el export a excel\n      activePrompt: false,\n      codigoSelected: [],\n      reportesActivo: 0,\n      fileName: null,\n      formats: ['xls', 'xlsx', 'csv', 'txt'],\n      cellAutoWidth: true,\n      selectedFormat: 'xlsx',\n      permisoRoot: false,\n      //fin variables para el export a excel\n      //variables para ver el historico\n      popupHistorico: false,\n      codigo: {}\n      //fin variables para ver el historico\n    };\n  },\n  components: {\n    ListadoDevolucionesCodigosVue: ListadoDevolucionesCodigosVue,\n    historicoCodigosVue: historicoCodigosVue\n  },\n  mounted: function mounted() {\n    var me = this;\n    me.usuario = JSON.parse(localStorage.getItem(\"usuario\"));\n    me.getUserRoot();\n    // me.arregloCodigos.forEach(p => {\n    //   if(p.bc_estado == 1){\n    //     p.pruebas = \"good\"\n    //   }else{\n    //     p.pruebas = \"bad\"\n    //   }\n    // });\n  },\n  methods: {\n    getUserRoot: function getUserRoot() {\n      var me = this;\n      this.$http.get(this.$server_url + 'permisos?permiso=yes&usuario_id=' + me.usuario.idusuario).then(function (res) {\n        if (res.data.status == '1') {\n          me.permisoRoot = true;\n        } else {\n          me.permisoRoot = false;\n        }\n      }).catch(function (error) {\n        console.log(error + ' error');\n      });\n    },\n    openEditCodigo: function openEditCodigo(tr) {\n      var me = this;\n      var datos = {\n        \"codigosEdit\": tr,\n        \"nuevo\": true,\n        \"editar\": 1,\n        \"eliminar\": 0\n      };\n      me.$emit('crud', datos);\n    },\n    openConfirm: function openConfirm(tr) {\n      var me = this;\n      var datos = {\n        \"id\": tr.codigo,\n        \"eliminar\": 1\n      };\n      me.$emit('crud', datos);\n    },\n    //methods to return codes\n    returnCodes: function returnCodes(tr) {\n      var me = this;\n      me.titleReturns = \"Informacion de devoluciones del codigo \" + tr.codigo;\n      me.$vs.loading();\n      this.$http.get(this.$server_url + 'codigo/devoluciones/' + tr.codigo).then(function (res) {\n        me.$vs.loading.close();\n        me.devoluciones = res.data;\n        me.popupReturns = true;\n      }).catch(function (e) {\n        me.$vs.loading.close();\n      });\n    },\n    editarCodigoEstudiante: function editarCodigoEstudiante(tr) {\n      var me = this;\n      var datos = {\n        \"info\": tr,\n        \"tipo\": \"editar\"\n      };\n      me.$emit('crud', datos);\n    },\n    ConfirmCambiadoEstadoCodigo: function ConfirmCambiadoEstadoCodigo(tr, estado) {\n      var me = this;\n      var datos = {\n        \"info\": tr,\n        \"tipo\": \"bloquear\",\n        \"estado\": estado\n      };\n      me.$emit('crud', datos);\n    },\n    //METODOS PARA EXCEL\n    //para limipiar los campos del excel\n    clearFields: function clearFields() {\n      this.fileName = '';\n      this.cellAutoWidth = true;\n      this.selectedFormat = 'xls';\n    },\n    //metodo par descargar el excel\n    exportToExcel: function exportToExcel() {\n      var _this = this;\n      var headerTitle = ['codigo', 'Codigo union', 'Paquete', 'Devolucion', 'institucion estudiante', 'periodo estudiante', 'estudiante', 'cedula', 'estado', 'liquidacion', 'código de barras', 'bc_estado', 'venta estado', 'institucion barras', 'periodo barras', 'institucion lista', 'Tipo código'];\n      var headerVal = ['codigo', 'codigo_union', 'codigo_paquete', 'devolucionInstitucion', 'nombreInstitucion', 'periodo', 'estudiante', 'cedula', 'codigoEstado', 'liquidacion', 'status', 'barrasEstado', 'ventaEstado', 'institucionBarra', 'periodo_barras', 'InstitucionLista', 'tipoCodigo'];\n      var codigoSelectedMayus = this.arregloCodigos;\n      for (var i = 0; i < codigoSelectedMayus.length; i++) {\n        for (var key in codigoSelectedMayus[i]) {\n          if (typeof codigoSelectedMayus[i][key] == \"string\") {\n            codigoSelectedMayus[i][key] = codigoSelectedMayus[i][key].toUpperCase();\n          }\n        }\n      }\n      import('@/vendor/Export2Excel').then(function (excel) {\n        var list = codigoSelectedMayus;\n        var data = _this.formatJson(headerVal, list);\n        var nombrefile = \"\";\n        if (_this.openModal == 1) {\n          nombrefile = _this.fileName;\n        } else {\n          nombrefile = \"Reporte de codigo\";\n        }\n        excel.export_json_to_excel({\n          header: headerTitle,\n          data: data,\n          filename: nombrefile,\n          autoWidth: _this.cellAutoWidth,\n          bookType: _this.selectedFormat\n        });\n        _this.clearFields();\n        _this.guardarActivo = '';\n        _this.generarActivo = ' ';\n        _this.textAlertCant = '';\n        _this.cantidad = '';\n        _this.codigoSelected = [];\n      });\n    },\n    formatJson: function formatJson(filterVal, jsonData) {\n      return jsonData.map(function (v) {\n        return filterVal.map(function (j) {\n        