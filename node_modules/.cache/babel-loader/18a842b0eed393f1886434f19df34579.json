],\n      attachmentArray: [],\n      sendTarea: {\n        id: 0,\n        mensaje: '',\n        estado: 0\n      },\n      attachmentArray2: [],\n      arregloRespuestas: [],\n      userEstudiante: false,\n      userDocente: false\n    };\n  },\n  components: {\n    quillEditor: quillEditor,\n    DetalleTareasEnviadasVue: DetalleTareasEnviadasVue,\n    CalificacionesRecursosVue: CalificacionesRecursosVue\n  },\n  created: function created() {\n    var me = this;\n    me.usuario = JSON.parse(localStorage.getItem(\"usuario\"));\n  },\n  mounted: function mounted() {\n    var me = this;\n    if (me.usuario.id_group == 4) me.userEstudiante = true;\n    if (me.usuario.id_group == 6) me.userDocente = true;\n    me.getRecursosFiles();\n    me.getInfoTarea();\n  },\n  props: {\n    tarea: {\n      type: Object,\n      default: {}\n    },\n    codigoCurso: {\n      type: String,\n      default: ''\n    }\n  },\n  methods: {\n    getInfoTarea: function getInfoTarea() {\n      var me = this;\n      var datos = me.usuario.idusuario + \"*\" + me.tarea.id;\n      this.$http.get(this.$server_url + 'neetTema?getInfoTarea=yes&datos=' + datos).then(function (res) {\n        if (res.data.length > 0) {\n          var _datos = res.data[0];\n          me.sendTarea.id = _datos.id;\n          me.sendTarea.estado = _datos.estado;\n          me.getRespuestasTareas();\n        } else {\n          me.sendTarea.id = 0;\n          me.sendTarea.estado = 0;\n        }\n      }).catch(function (error) {\n        console.log(error + ' error');\n        me.$vs.loading.close();\n      });\n    },\n    irEnlace: function irEnlace(url) {\n      var me = this;\n      window.open(url, '_blank');\n    },\n    getRecursosFiles: function getRecursosFiles() {\n      var me = this;\n      me.attachmentArray = [];\n      this.$http.get(this.$server_url + 'neetTema/' + me.tarea.neet_upload_id).then(function (res) {\n        //archivos\n        if (res.data.files.length > 0) {\n          var files = res.data.files;\n          if (files.length > 0) {\n            files.forEach(function (element) {\n              var cal = new Object();\n              cal.id = element.id;\n              cal.name = element.archivo;\n              cal.url = element.url;\n              var tipo = \"\";\n              if (element.ext == \"PNG\" || element.ext == \"png\" || element.ext == \"JPEG\" || element.ext == \"jpeg\" || element.ext == \"JPG\" || element.ext == \"jpg\") {\n                tipo = \"img\";\n              } else {\n                tipo = \"other\";\n              }\n              cal.tipo = tipo;\n              //local 0 = local; 1 => de la base\n              cal.local = 1;\n              me.attachmentArray.push(cal);\n            });\n          }\n        }\n      }).catch(function (error) {\n        console.log(error + ' error');\n      });\n    },\n    //METODOS FILES\n    mostrarImg: function mostrarImg(e) {\n      var me = this;\n      var selectedFiles = e.target.files;\n      for (var i = 0; i < selectedFiles.length; i++) {\n        var archivo = selectedFiles[i];\n        me.setFile(archivo);\n      }\n    },\n    setFile: function setFile(archivo) {\n      var me = this;\n      var tipo = \"\";\n      if (archivo.type == \"image/png\" || archivo.type == \"image/gif\" || archivo.type == \"image/jpeg\" || archivo.type == \"image/jpg\") {\n        tipo = \"img\";\n      } else {\n        tipo = \"other\";\n      }\n      var reader = new FileReader();\n      if (me) {\n        reader.readAsDataURL(archivo);\n        reader.onloadend = function () {\n          var cal = new Object();\n          cal.file = archivo;\n          cal.img = reader.result;\n          cal.tipo = tipo;\n          cal.name = archivo.name;\n          //local 0 ; 1 de la base\n          cal.local = 0;\n          me.attachmentArray2.push(cal);\n        };\n      }\n      return me.attachmentArray2;\n    },\n    guadarTarea: function guadarTarea() {\n      var me = this;\n      if (me.sendTarea.mensaje == \"\") {\n        me.$vs.notify({\n          text: 'La descripción de la tarea no puede estar vacia',\n          color: 'warning',\n          iconPack: 'feather',\n          icon: 'icon-alert-triangle'\n        });\n        return;\n      }\n      if (me.sendTarea.mensaje.length < 5) {\n        me.$vs.notify({\n          text: 'La descripción de la tarea no puede menor a 5 caracteres',\n          color: 'warning',\n          iconPack: 'feather',\n          icon: 'icon-alert-triangle'\n        });\n        return;\n      }\n      //files\n      var imgsEnviar = [];\n      me.attachmentArray2.forEach(function (element) {\n        imgsEnviar.push(element.file);\n      });\n      var formData = new FormData();\n      formData.append('id', me.sendTarea.id);\n      formData.append('periodo_id', localStorage.periodo_id);\n      formData.append('neet_cursos_has_material_id', me.tarea.id);\n      formData.append('idusuario', me.usuario.idusuario);\n      formData.append('mensaje', me.sendTarea.mensaje);\n      formData.append('curso', me.codigoCurso);\n      for (var i = 0; i < this.attachmentArray2.length; i++) {\n        formData.append('archivos[]', imgsEnviar[i]);\n      }\n      var config = {\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      };\n      me.$vs.loading();\n      axios.post(this.$data_url + 'api/guardarTareaNeet', formData, config).then(function (res) {\n        me.$vs.loading.close();\n        me.$vs.notify({\n          text: \"Se guardo correctamente\",\n          color: 'success',\n          iconPack: 'feather',\n          icon: 'icon-check'\n        });\n        document.getElementById(\"file1Recurso\").value = '';\n        me.attachmentArray2 = [];\n        me.sendTarea.mensaje = \"\";\n        me.getInfoTarea();\n      }).catch(function (error) {\n        me.$vs.loading.close();\n      });\n    },\n    getRespuestasTareas: function getRespuestasTareas() {\n      var me = this;\n      this.$http.get(this.$server_url + 'neetTema?listadoRespuestas=yes&tarea_id=' + me.sendTarea.id).then(function (res) {\n        me.arregloRespuestas = res.data;\n      }).catch(function (error) {\n        console.log(error + ' error');\n      });\n    },\n    removeItemFromArr: function removeItemFromArr(arr, item) {\n      var me = this;\n      var i = arr.indexOf(item);\n      arr.splice(i, 1);\n      if (item.local == 1) {\n        me.deleteOnlyFile(item.id);\n      }\n    },\n    deleteOnlyFile: function deleteOnlyFile(id) {\n      var me = this;\n      axios.get(this.$data_url + 'api/deleteFileRecurso/' + id).then(function (res) {}).catch(function (error) {\n        console.log(error);\n      });\n    },\n    descargar_archivo: function descargar_archivo(item) {\n      var me = this;\n      window.open(this.$data_url + 'archivos/neetFiles/tareas' + item.url, '_blank');\n    },\n    verActividad: function verActividad(url) {\n      var me = this;\n      localStorage.Exteriorurl = url;\n      var route = this.$router.resolve({\n        path: '/admin_neet_actividad'\n      });\n      window.open(route.href, '_blank');\n    }\n  }\n};",{"version":3,"names":["DetalleTareasEnviadasVue","CalificacionesRecursosVue","axios","quillEditor","data","usuario","attachmentArray","sendTarea","id","mensaje","estado","attachmentArray2","arregloRespuestas","userEstudiante","userDocente","components","created","me","JSON","parse","localStorage","getItem","mounted","id_group","getRecursosFiles","getInfoTarea","props","tarea","type","Object","default","codigoCurso","String","methods","datos","idusuario","$http","get","$server_url","then","res","length","getRespuestasTareas","catch","error","console","log","$vs","loading","close","irEnlace","url","window","open","neet_upload_id","files","forEach","element","cal","name","archivo","tipo","ext","local","push","mostrarImg","e","selectedFiles","target","i","setFile","reader","FileReader","readAsDataURL","onloadend","file","img","result","guadarTarea","notify","text