be_orden_firmada);\n        cal.diferencia10 = lapso10;\n        me.datosMostrar.push(lapso10);\n        //enviar datos\n        me.setRender(me.datosMostrar);\n        //this.series[0].data = me.datosMostrar;\n        me.tmpArreglo.push(cal);\n        me.arregloTransabilidad = me.tmpArreglo[0];\n      }).catch(function (error) {\n        console.log(error + ' error');\n        me.$vs.loading.close();\n      });\n    },\n    setRender: function setRender(tr) {\n      var me = this;\n      var arreglo = tr;\n      this.series[0].data = arreglo;\n    },\n    calculateHours: function calculateHours(fecha_mayor, fecha_menor) {\n      if (fecha_mayor == null || fecha_menor == null) {\n        return 0;\n      }\n      {\n        var fecha1 = new Date(Date.parse(fecha_menor.replace(' ', 'T')));\n        var fecha2 = new Date(Date.parse(fecha_mayor.replace(' ', 'T')));\n        var diferenciaEnMilisegundos = fecha2 - fecha1;\n        // const diferenciaEnMilisegundos = Math.abs(fecha2 - fecha1);\n        var diferenciaEnHoras = Math.floor(diferenciaEnMilisegundos / 3600000);\n        return diferenciaEnHoras;\n      }\n    },\n    //metodo par descargar el excel\n    ExportToExcel: function ExportToExcel(type, fn, dl) {\n      var elt = document.getElementById('tbl_exporttable_to_xls');\n      var wb = XLSX.utils.table_to_book(elt, {\n        sheet: \"sheet1\"\n      });\n      return dl ? XLSX.write(wb, {\n        bookType: type,\n        bookSST: true,\n        type: 'base64'\n      }) : XLSX.writeFile(wb, fn || 'ReporteTareas.' + (type || 'xlsx'));\n    }\n  }\n};",{"version":3,"names":["chart","moment","components","apexchart","data","arregloTransabilidad","tmpArreglo","datosMostrar","series","name","options","height","type","plotOptions","bar","borderRadius","dataLabels","position","enabled","formatter","val","offsetY","style","fontSize","colors","xaxis","categories","axisBorder","show","axisTicks","crosshairs","fill","gradient","colorFrom","colorTo","stops","opacityF