];\n      me.portada = me.datosEditar.portada;\n      me.estado = me.datosEditar.estado;\n    },\n    volver: function volver() {\n      var me = this;\n      if (me.edita) {\n        localStorage.removeItem('publicacion');\n        localStorage.removeItem('edita');\n      }\n      me.id_pub = '';\n      me.nombre = '';\n      me.descripcion = '';\n      me.asignaturaSelect = '';\n      me.archivo = '';\n      me.portada = '';\n      me.estado = '';\n      this.$router.go(-1);\n    },\n    descargaArchivo: function descargaArchivo(item) {\n      var _this = this;\n      console.log(item);\n      return;\n      axios({\n        url: 'https://foro.prolipadigital.com.ec' + item.url,\n        method: 'GET',\n        responseType: 'blob'\n      }).then(function (response) {\n        var fileURL = window.URL.createObjectURL(new Blob([response.data]));\n        var fileLink = document.createElement('a');\n        fileLink.href = fileURL;\n        fileLink.setAttribute('download', item.name + item.ext);\n        document.body.appendChild(fileLink);\n        fileLink.click();\n        _this.$vs.notify({\n          text: 'El archivo se esta descargando',\n          color: 'success',\n          iconPack: 'feather',\n          icon: 'icon-check'\n        });\n      }).catch(function (error) {\n        _this.$vs.notify({\n          text: 'Error en la descarga del archivo',\n          color: 'warning',\n          iconPack: 'feather',\n          icon: 'icon-alert-triangle'\n        });\n      });\n    },\n    openConfirmEliminar: function openConfirmEliminar(item) {\n      var me = this;\n      // me.archivo = item\n      me.id_eliminar = item.id;\n      me.$vs.dialog({\n        type: 'confirm',\n        color: 'danger',\n        title: 'Confirmar',\n        text: '¿Está seguro de eliminar este archivo?',\n        acceptText: 'Aceptar',\n        cancelText: 'Cancelar',\n        accept: me.eliminarArchivo\n      });\n    },\n    eliminarArchivo: function eliminarArchivo() {\n      var me = this;\n      // console.log(me.tipo)\n      me.$vs.loading();\n      axios.delete(\"https://foro.prolipadigital.com.ec/upload/files/\" + me.id_eliminar).then(function (res) {\n        me.$vs.notify({\n          text: 'Archivo eliminado correctamente.',\n          color: 'success',\n          iconPack: 'feather',\n          icon: 'icon-check'\n        });\n        me.porcentaje = 0;\n        me.tipo == 'portada' ? me.portada = '' : me.archivo = '';\n        me.$vs.loading.close();\n      }).catch(function (error) {\n        me.$vs.notify({\n          text: 'No se eliminó el archivo',\n          color: 'warning',\n          iconPack: 'feather',\n          icon: 'icon-alert-triangle'\n        });\n        me.$vs.loading.close();\n      });\n    }\n  }\n};",{"version":3,"names":["Vue","axios","vSelect","components","data","asignaturas","asignaturaSelect","archivo","portada","porcentaje","id_pub","nombre","descripcion","estado","edita","datosEditar","porcentajep","limiteMegas","limitePortada","bytes","pesoArchivo","muyPesado","pesoArchivop","muyPesadop","idusuario","filep","id_eliminar","tipo","mounted","localStorage","getItem","usuario","JSON","parse","pasarDatos","methods","getAsignaturas","me","$http","get","$server_url","then","res","catch","error","console","log","handleFileUpload","file","$refs","files","size","toFixed","portadaUpload","subirArchivo","okArchivo","okPortada","valor","$vs","notify","text","color","iconPack","icon","loading","close","formData","FormData","append","post","onUploadProgress","uploadEvent","Math","round","loaded","total","response","subirPortada","formData1","guardaEdita","editarPublicacion","registrarPublicacion","id","asignatura","volver","put","removeItem","$router","go","descargaArchivo","item","_this","url","method","responseType","fileURL","window","URL","createObjectURL","Blob","fileLink","document","createElement","href","setAttribute","name","ext","body","appendChild","click","openConfirmEliminar","dialog","type","title"