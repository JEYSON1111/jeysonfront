n                if( me.usuario.id_group == 22 || me.usuario.id_group == 23 || me.usuario.id_group == 1 ){\r\n                    me.arregloGuias = datos.filter(p => p.pedido_facturacion == 1)\r\n                    return\r\n                }\r\n                if( me.usuario.id_group == 17 || me.usuario.id_group == 27 ){\r\n                    me.arregloGuias = datos.filter(p => p.pedido_bodega == 1)\r\n                    return\r\n                }\r\n                if( me.usuario.id_group == 11 || me.usuario.id_group == 11 ){\r\n                    me.arregloGuias = datos.filter(p => p.pedido_asesor == 1)\r\n                    return\r\n                }\r\n            })\r\n        },\r\n        getPeriodos(){\r\n            let me = this\r\n            this.$http.get(this.$server_url+'get_periodos_pedidos')\r\n            .then(res => {\r\n                let datos = res.data\r\n                if(datos.length == 0){\r\n                    me.periodos = []\r\n                }\r\n                if( me.usuario.id_group == 22 || me.usuario.id_group == 23 || me.usuario.id_group == 1 ){\r\n                    me.arregloPeriodos = datos.filter(p => p.pedido_facturacion == 1)\r\n                }\r\n                if( me.usuario.id_group == 17 || me.usuario.id_group == 27 ){\r\n                    me.arregloPeriodos = datos.filter(p => p.pedido_bodega == 1)\r\n                }\r\n                if( me.usuario.id_group == 11 || me.usuario.id_group == 11 ){\r\n                    me.arregloPeriodos = datos.filter(p => p.pedido_asesor == 1)\r\n                }\r\n            })\r\n        },\r\n        goToGuias(){\r\n            let me = this;\r\n            if(me.periodoSelect == \"\" || me.periodoSelect == null || me.periodoSelect == undefined){\r\n                me.$vs.notify({\r\n                text:'Seleccione un periodo por favor',\r\n                color:'warning',\r\n                iconPack: 'feather',\r\n                icon:'icon-check'})\r\n                return\r\n            }\r\n            me.popupPeriodos = false;\r\n            //buscar periodo sierra asignado a bodega\r\n            // localStorage.id_pedido = ''\r\n            localStorage.setItem(\"id_periodo_selected\", this.periodoSelect.idperiodoescolar)\r\n            localStorage.setItem(\"nombre_periodo_selected\", this.periodoSelect.periodoescolar)\r\n            let datos = {\r\n                id_pedido:0,\r\n            }\r\n            this.$store.dispatch('setGuia',datos)\r\n            me.menu = 1\r\n            // this.$router.push('/pedidos/guias/bodega/0')\r\n        },\r\n        changeHandleRecarga(e){\r\n            let me = this;\r\n            me.getGuias()\r\n        }\r\n    },\r\n   \r\n}\r\n</script>\r\n"],"mappings":"AAgDA,IAAAA,yBAAA,YAAAA,0BAAA;EAAA;AAAA;AACA,IAAAC,mBAAA,YAAAA,oBAAA;EAAA;AAAA;AACA,IAAAC,4BAAA,YAAAA,6BAAA;EAAA;AAAA;AACA,IAAAC,wBAAA,YAAAA,yBAAA;EAAA;AAAA;AACA,IAAAC,eAAA,YAAAA,gBAAA;EAAA;AAAA;AACA,OAAAC,OAAA;AACA;EACAC,UAAA;IACAL,mBAAA,EAAAA,mBAAA;IACAC,4BAAA,EAAAA,4BAAA;IACAF,yBAAA,EAAAA,yBAAA;IACAK,OAAA,EAAAA,OAAA;IACAF,wBAAA,EAAAA,wBAAA;IACAC,eAAA,EAAAA;EACA;EACAG,IAAA,WAAAA,KAAA;IACA;MACAC,OAAA;MACAC,eAAA;MACAC,YAAA;MACAC,aAAA;MACAC,aAAA;MACAC,WAAA;MACAC,IAAA;IACA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,IAAAC,EAAA;IACAA,EAAA,CAAAR,OAAA,GAAAS,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;EACA;EACAC,OAAA,WAAAA,QAAA;IACA,IAAAL,EAAA;IACAA,EAAA,CAAAM,QAAA;IACAN,EAAA,CAAAO,WAAA;EACA;EACAC,KAAA;IACAZ,aAAA,WAAAA,cAAAa,OAAA;MACA,IAAAT,EAAA;MACA,IAAAS,OAAA,UAAAA,OAAA,YAAAA,OAAA,IAAAC,SAAA,IAAAD,OAAA;QACAT,EAAA,CAAAH,WAAA;MACA;QACAG,EAAA,CAAAH,WAAA;MACA;IACA;EACA;EACAc,OAAA;IACAC,UAAA,WAAAA,WAAAC,CAAA;MACA,IAAAb,EAAA;MACAA,EAAA,CAAAF,IAAA,GAAAe,CAAA;IACA;IACAP,QAAA,WAAAA,SAAA;MACA,IAAAN,EAAA;MACAA,EAAA,CAAAN,YAAA;MACA,KAAAoB,KAAA,CAAAC,GAAA,MAAAC,WAAA,wBACAC,IAAA,WAAAC,GAAA;QACA,IAAAC,KAAA,GAAAD,GAAA,CAAA3B,IAAA;QACA,IAAA4B,KAAA,CAAAC,MAAA;UACApB,EAAA,CAAAN,YAAA;UACA;QACA;QACA,IAAAM,EAAA,CAAAR,OAAA,CAAA6B,QAAA,UAAArB,EAAA,CAAAR