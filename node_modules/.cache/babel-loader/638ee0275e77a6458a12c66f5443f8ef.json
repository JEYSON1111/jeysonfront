me.cuaderno = me.permisosExternos.permisos_cuadernos;\n      me.planificacion = me.permisosExternos.permisos_planificaciones;\n      //data de los recursos adicionales\n      me.mijuego = me.permisosExternos.zona_diversion_mi_juego;\n      me.juegoProlipa = me.permisosExternos.zona_diversion_juego_prolipa;\n      me.materialDigital = me.permisosExternos.material_apoyo_digital;\n      me.materialpdf = me.permisosExternos.material_apoyo_pdf;\n      me.propuesta = me.permisosExternos.propuestas_metodologicas;\n      me.adaptacion = me.permisosExternos.adaptaciones;\n      me.articulo = me.permisosExternos.articulos;\n      me.documentom = me.permisosExternos.documentos_ministeriales;\n    }\n    //FIN DE PERMISOS EXTERNOS\n  },\n  methods: {\n    getAdaptaciones: function getAdaptaciones() {\n      var me = this;\n      me.$vs.loading();\n      if (me.grupo_usuario == 1) {\n        //todas las propuestas metodologicas, para los administradores\n        this.$http.get(this.$server_url + 'adaptaciones?admin=yes').then(function (res) {\n          me.$vs.loading.close();\n          var datos = res.data;\n          for (var i = 0; i < datos.length; i++) {\n            //files\n            var fUnidad1 = [];\n            var fUnidad2 = [];\n            var fUnidad3 = [];\n            var fUnidad4 = [];\n            var fUnidad5 = [];\n            var fUnidad6 = [];\n            fUnidad1 = datos[i].files.filter(function (p) {\n              return p.unidad == 1;\n            });\n            fUnidad2 = datos[i].files.filter(function (p) {\n              return p.unidad == 2;\n            });\n            fUnidad3 = datos[i].files.filter(function (p) {\n              return p.unidad == 3;\n            });\n            fUnidad4 = datos[i].files.filter(function (p) {\n              return p.unidad == 4;\n            });\n            fUnidad5 = datos[i].files.filter(function (p) {\n              return p.unidad == 5;\n            });\n            fUnidad6 = datos[i].files.filter(