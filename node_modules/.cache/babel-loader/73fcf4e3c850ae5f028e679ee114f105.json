,\n      nuevoUser: \"si\",\n      parametros: {\n        id: '',\n        institucion: ''\n      },\n      user: {\n        idusuario: 0,\n        cedula: '',\n        nombres: '',\n        apellidos: '',\n        email: '',\n        telefono: '',\n        periodoDescripcion: '',\n        nombreInstitucion: '',\n        periodo_id: '',\n        encuesta_id: ''\n      },\n      libro_docente_id: 0\n    };\n  },\n  mixins: [historicoMixin],\n  created: function created() {\n    var me = this;\n    me.parametros.id = this.$route.params.id;\n    me.parametros.institucion = this.$route.params.institucion;\n    me.validateForm();\n  },\n  mounted: function mounted() {\n    var me = this;\n    if (localStorage.tk != null) {\n      this.$router.push('/home');\n    } else {}\n    me.getSeries();\n  },\n  methods: {\n    //validate Form\n    validateForm: function validateForm() {\n      var me = this;\n      this.$http.get(this.$server_url + 'encuesta?validarEncuesta=yes&institucion_id=' + me.parametros.institucion + '&id=' + me.parametros.id).then(function (res) {\n        if (res.data.status == 0) {\n          me.validate = false;\n          me.msgError = res.data.message;\n          return;\n        } else {\n          me.validate = true;\n          me.user.periodoDescripcion = res.data.periodoDescripcion;\n          me.user.nombreInstitucion = res.data.nombreInstitucion[0].nombreInstitucion;\n          me.user.periodo_id = res.data.periodo_id;\n          me.user.encuesta_id = res.data.encuesta_id;\n        }\n      }).catch(function (error) {\n        console.log(error + ' error');\n      });\n    },\n    //para obtener  la series\n    getSeries: function getSeries() {\n      var me = this;\n      this.$http.get(this.$server_url + 'traerSeries?docentes=yes&formulario_id=' + me.parametros.id).then(function (res) {\n        me.arregloManantial = res.data.filter(function (p) {\n          return p.id_serie == 1 || p.id_serie == 2;\n        });\n        me.arregloRuta = res.