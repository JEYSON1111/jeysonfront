or_id: 0,\n      turno: 0\n    };\n  },\n  created: function created() {\n    var me = this;\n    me.usuario = JSON.parse(localStorage.getItem(\"usuario\"));\n  },\n  mounted: function mounted() {\n    var me = this;\n  },\n  components: {\n    vSelect: vSelect\n  },\n  methods: {\n    changehandleUpdate: function changehandleUpdate(e) {\n      var me = this;\n      me.getComunicados();\n      me.menu = 0;\n    },\n    changehandleOpenComunicado: function changehandleOpenComunicado(e) {\n      var me = this;\n      me.ComunicadoEnviar = e.comunicado;\n      me.menu = 2;\n    },\n    getInstituciones: function getInstituciones() {\n      var me = this;\n      this.$http.get(this.$server_url + 'listaInsitucion?busqueda').then(function (res) {\n        me.instituciones = res.data;\n        if (res.data.message) {\n          me.$vs.notify({\n            text: res.data.message,\n            color: 'success',\n            iconPack: 'feather',\n            icon: 'icon-user'\n          });\n        }\n        //incrustar la institucion del administrador por defecto\n        var datos = me.instituciones.filter(function (p) {\n          return p.idInstitucion == me.usuario.institucion_idInstitucion;\n        });\n        me.institucion_General = datos[0];\n        me.getComunicados();\n      }).catch(function (error) {\n        console.log(error + ' error');\n      });\n    },\n    setTeachers: function setTeachers(tr) {\n      var me = this;\n      var valores = [];\n      var p = new Promise(function (resolve, reject) {\n        if (tr.length) {\n          resolve(tr);\n        } else {\n          reject('No hay comunicados de profesores');\n        }\n      });\n      p.then(function (res) {\n        var datos = res;\n        datos.forEach(function (element) {\n          var cal = new Object();\n          cal.docente = element.emisor;\n          cal.receptor_id = element.user_created;\n          valores.push(cal);\n        });\n        //obtener contador por docente\n     