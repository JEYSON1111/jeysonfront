ault-button\" size=\"small\" color=\"success\" @click=\"$emit('changeRecharge',2,tr)\" type=\"filled\" icon-pack=\"feather\" icon=\"icon-edit\"> Editar</vs-button>\r\n            </vx-tooltip> &nbsp;\r\n            <vx-tooltip style=\"display: inline-block;\" text=\"Eliminar contrato\" position=\"top\" color=\"primary\">\r\n              <vs-button class=\"modal-default-button\" size=\"small\" color=\"danger\" @click=\"convenioSelect=tr;menu = 1;\" type=\"filled\" icon-pack=\"feather\" icon=\"icon-x-square\">Eliminar </vs-button>\r\n            </vx-tooltip> &nbsp;\r\n          </vs-td>\r\n        </vs-tr>\r\n      </template>\r\n    </vs-table>\r\n    <vx-card v-if=\"menu == 1\" :title=\"'Eliminar contrato '+convenioSelect.ven_codigo\">\r\n      <ConfirmEliminarVue @changeConfirm=\"changeConfirm\"/>\r\n    </vx-card>\r\n  </div>\r\n</template>\r\n<script>\r\nconst ConfirmEliminarVue = () => import('../../../../../configuracion/Reutilizable/ConfirmEliminar.vue')\r\n// import ConfirmEliminarVue from '../../../../../configuracion/Reutilizable/ConfirmEliminar.vue';\r\nexport default {\r\n  components:{\r\n    ConfirmEliminarVue,\r\n  },\r\n  data(){\r\n    return{\r\n      menu:0,\r\n      convenioSelect:'',\r\n    }\r\n  },\r\n  props:{\r\n    usuario:{\r\n      type:Object,\r\n      default:function(){\r\n        return {}\r\n      }\r\n    },\r\n    idConvenio:{\r\n      type:Number,\r\n      default:0,\r\n    },\r\n    arrayAgreements:{\r\n      type:Array,\r\n      default:function(){\r\n        return []\r\n      }\r\n    },\r\n    showSuggested:{\r\n      type:Boolean,\r\n      default:false,\r\n    },\r\n    setAnticipoGlobal:{\r\n      type:Number,\r\n      validator(value){\r\n      return value%1!==0\r\n      }\r\n    },\r\n    groupFacturador:{\r\n      type:Boolean,\r\n      default:false,\r\n    },\r\n    datosInstitucion:{\r\n      type:Object,\r\n      default:function(){\r\n        return {}\r\n      }\r\n    }\r\n  },\r\n  watch:{\r\n    showSuggested