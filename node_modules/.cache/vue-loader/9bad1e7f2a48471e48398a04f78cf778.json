to=yes&factura='+me.facturaSelect.factura)\r\n      .then(function (res) {\r\n        me.$vs.loading.close()\r\n        if(res.data.length == 0) {\r\n          me.$vs.notify({\r\n          text:'No se encontro resultados',\r\n          color:'warning',\r\n          iconPack: 'feather',\r\n          icon:'icon-check'})\r\n          return\r\n        }\r\n        me.arregloCodigos     = res.data\r\n        me.popupSelectFactura = false\r\n      })\r\n      .catch(function (error) {\r\n        me.$vs.loading.close()\r\n        console.log(error + ' error');\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n"]}]}