    this.$http.post(this.$server_url+'calificaciones_material_curso', formData)\r\n                .then(res => {\r\n                    me.calificaciones = res.data;\r\n                    me.$vs.loading.close()\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                    me.$vs.loading.close()\r\n                })\r\n        },\r\n    }\r\n}\r\n",{"version":3,"sources":["calificaciones_materialapoyo.vue"],"names":[],"mappings":";AA4CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"calificaciones_materialapoyo.vue","sourceRoot":"src/views/materialapoyo","sourcesContent":["<template>\r\n<div>\r\n<vx-card>\r\n    <vs-button style=\"display:inline-block\" @click=\"$router.go(-1);\" color=\"dark\" type=\"border\" class=\"mr-4\"><b>← Volver</b></vs-button>\r\n    <h4 style=\"display:inline-block\">\r\n        <b>Curso:</b> {{nombre_curso}} - <b>Material:</b> {{nombre_material}}\r\n    </h4>\r\n<div v-if=\"calificaciones.length > 0\">\r\n    <vs-table stripe pagination max-items=\"30\" search :data=\"calificaciones\">\r\n        <template slot=\"thead\">\r\n            <vs-th style=\"max-width: 20px;\">#</vs-th>\r\n            <vs-th>Estudiante</vs-th>\r\n            <vs-th>Cédula</vs-th>\r\n            <vs-th>Calificación</vs-th>\r\n        </template>\r\n\r\n        <template slot-scope=\"{data}\">\r\n            <vs-tr :data=\"tr\" :key=\"indextr\" v-for=\"(tr, indextr) in data\">\r\n                <td>\r\n                    {{ indextr+1 }}\r\n                </td>\r\n                <vs-td>\r\n                    {{ data[indextr].nombres }} {{ data[indextr].apellidos }} <br>\r\n                    {{ data[indextr].email }}\r\n                </vs-td>\r\n                <vs-td width=\"30%\">\r\n                    {{ data[indextr].cedula }}\r\n                </vs-td>\r\n                <vs-td>\r\n                    {{ data[indextr].calificacion }}\r\n                </vs-td>\r\n            </vs-tr>\r\n        </template>\r\n    </vs-table>\r\n</div>\r\n<div v-else>\r\n    Ninguna calificacion para este material todavía.\r\n</div>\r\n\r\n    </vx-card>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport Vue from 'vue'\r\nimport axios from 'axios'\r\nimport vSelect from 'vue-select'\r\nexport default {\r\n    components: {\r\n        'v-select': vSelect,\r\n    },\r\n    data() {\r\n        return {\r\n            calificaciones: [],\r\n            nombre_curso: '',\r\n            nombre_material: '',\r\n        }\r\n    },\r\n    mounted() {\r\n        let me = this;\r\n        me.usuario = JSON.parse(localStorage.getItem('usuario'));\r\n        me.nombre_curso = localStorage.nombre_curso_calif\r\n        me.nombre_material = localStorage.nombre_material_calif\r\n        me.getCalificaciones();\r\n    },\r\n    methods: {\r\n        getCalificaciones() {\r\n            let me = this;\r\n            me.$vs.loading()\r\n            let formData = new FormData();\r\n            formData.append('codigo_curso', localStorage.codigo_calif);\r\n            formData.append('id_material', localStorage.id_material_calif);\r\n            this.$http.post(this.$server_url+'calificaciones_material_curso', formData)\r\n                .then(res => {\r\n                    me.calificaciones = res.data;\r\n                    me.$vs.loading.close()\r\n                })\r\n                .catch(error => {\r\n                    console.log(error)\r\n                    me.$vs.loading.close()\r\n                })\r\n        },\r\n    }\r\n}\r\n</script>\r\n"]}]}